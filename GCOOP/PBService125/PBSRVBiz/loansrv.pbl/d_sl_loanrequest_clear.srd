         ~"LNREQLOANCLR~".~"PERIOD_PAYMENT~",   
         ~"LNREQLOANCLR~".~"PRINCIPAL_BALANCE~",   
         ~"LNREQLOANCLR~".~"WITHDRAWABLE_AMT~",   
         ~"LNREQLOANCLR~".~"LAST_PERIODPAY~",   
         ~"LNREQLOANCLR~".~"LASTCALINT_DATE~",   
         ~"LNREQLOANCLR~".~"CONTRACTINT_TYPE~",   
         ~"LNREQLOANCLR~".~"CONTRACT_INTEREST~",   
         ~"LNREQLOANCLR~".~"INTEREST_ARREAR~",   
         ~"LNREQLOANCLR~".~"INTESTIMATE_AMT~",   
         ~"LNREQLOANCLR~".~"RKEEP_PRINCIPAL~",   
         ~"LNREQLOANCLR~".~"RKEEP_INTEREST~",   
         ~"LNREQLOANCLR~".~"LASTPROCESS_DATE~",   
         ~"LNREQLOANCLR~".~"CONTRACT_STATUS~",   
         ~"LNREQLOANCLR~".~"CLEAR_STATUS~",   
         ~"LNREQLOANCLR~".~"MINPERIOD_PAY~",   
         ~"LNREQLOANCLR~".~"MINPERCENT_PAY~",   
         ~"LNREQLOANCLR~".~"INTCLEAR_AMT~",   
         ~"LNCONTMASTER~".~"LOANAPPROVE_DATE~",   
         ~"LNCONTMASTER~".~"STARTCONT_DATE~",   
         ~"LNREQLOANCLR~".~"INT_CONTINTTYPE~",   
         ~"LNREQLOANCLR~".~"INT_CONTINTRATE~",   
         ~"LNREQLOANCLR~".~"INT_CONTINTTABCODE~",   
         ~"LNREQLOANCLR~".~"INT_CONTINTINCREASE~",   
         ~"LNREQLOANCLR~".~"INT_INTSTEPTYPE~",   
         ~"LNREQLOANCLR~".~"INTRETURN_AMT~",   
         ~"LNREQLOANCLR~".~"PERIOD_PAYAMT~",   
         ~"LNREQLOANCLR~".~"CONTLAW_STATUS~",   
         ~"LNREQLOANCLR~".~"PAYMENT_STATUS~",   
         ~"LNREQLOANCLR~".~"PRINCIPAL_TRANSBAL~",   
         ~"LNREQLOANCLR~".~"INSURECOLL_FLAG~",   
