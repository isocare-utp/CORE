         ~"LNREQLOANCLR~".~"CONCOOP_ID~",   
         ~"LNCONTMASTER~".~"LOANPAY_CODE~"  
    FROM ~"LNREQLOANCLR~",   
         ~"LNLOANTYPE~",   
         ~"LNCONTMASTER~"  
   WHERE ( ~"LNREQLOANCLR~".~"LOANCONTRACT_NO~" = ~"LNCONTMASTER~".~"LOANCONTRACT_NO~" ) and  
         ( ~"LNREQLOANCLR~".~"LOANTYPE_CODE~" = ~"LNLOANTYPE~".~"LOANTYPE_CODE~" ) and  
         ( ~"LNLOANTYPE~".~"COOP_ID~" = ~"LNREQLOANCLR~".~"CONCOOP_ID~" ) and  
         ( ~"LNCONTMASTER~".~"COOP_ID~" = ~"LNREQLOANCLR~".~"CONCOOP_ID~" ) and  
         ( ( lnreqloanclr.loanrequest_docno = :as_reqno ) ) AND  
         ~"LNREQLOANCLR~".~"COOP_ID~" = :as_coopid    
" update="LNREQLOANCLR" updatewhere=0 updatekeyinplace=yes arguments=(("as_reqno", string),("as_coopid", string)) )
