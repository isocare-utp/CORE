	<queryString>
		<![CDATA[select
ns3.size_rai,ns3.size_ngan,ns3.size_wa,ns3.pos_moo,ns3.pos_tambol,ns3.pos_amphur,
ns3.pos_province,   ns3.ns3_docno,   ns3.ns3_bookno,   ns3.ns3_pageno,ns3.ns3_landno,
ns3.photoair_province,ns3.photoair_number,ns3.photoair_page ,
lnm.mrtg_name1, replace( to_char( lnm.mrtg_personid1 , '9,9999,99999,99,9' ) , ',' , '-' ) , lnm.mrtg_name2, lnm.mrtg_personid2, lnm.mrtg_age1, lnm.mrtg_nationality1, lnm.mrtg_citizenship1, lnm.mrtg_parentname1, lnm.mrtg_matename1,
lnm.mrtg_village1, lnm.mrtg_address1, lnm.mrtg_soi1, lnm.mrtg_road1, lnm.mrtg_moo1, lnm.mrtg_tambol1, lnm.mrtg_amphur1, lnm.mrtg_province1,
lnm.mortgage_grtname, lnm.autzd_name, lnm.autzd_age, lnm.autzd_nationality, lnm.autzd_citizenship, lnm.autzd_parentname,
lnm.autzd_village, lnm.autzd_address, lnm.autzd_moo, lnm.autzd_tambol, lnm.autzd_amphur, lnm.autzd_province,
lnm.mortgagesum_amt, ft_readtbaht(lnm.mortgagesum_amt), case lnm.mortgage_type when 0 then 1 when 2 then lnm.mortgage_landnum else null end,
case lnm.assettype_code when '03' then 'ก.' end, lnm.land_office, ftcnvtdate(lnm.mortgage_date, 3)
from lnmrtgmaster lnm,
(
select coop_id,
       mrtgmast_no,
       LISTAGG(size_rai, ', ') WITHIN GROUP (ORDER BY size_rai) as size_rai ,
       LISTAGG(size_ngan, ', ') WITHIN GROUP (ORDER BY size_ngan) as size_ngan,
       LISTAGG(size_wa, ', ') WITHIN GROUP (ORDER BY size_wa) as size_wa,
       LISTAGG(pos_moo, ', ') WITHIN GROUP (ORDER BY pos_moo) as pos_moo,
       LISTAGG(pos_tambol, ', ') WITHIN GROUP (ORDER BY pos_tambol) as pos_tambol,
       LISTAGG(pos_amphur, ', ') WITHIN GROUP (ORDER BY pos_amphur) as pos_amphur,
       LISTAGG(pos_province, ', ') WITHIN GROUP (ORDER BY pos_province) as pos_province,
       LISTAGG(ns3_docno, ', ') WITHIN GROUP (ORDER BY ns3_docno) as ns3_docno,
       LISTAGG(ns3_bookno, ', ') WITHIN GROUP (ORDER BY ns3_bookno) as ns3_bookno,
       LISTAGG(ns3_pageno, ', ') WITHIN GROUP (ORDER BY ns3_pageno) as ns3_pageno,
       LISTAGG(ns3_landno, ', ') WITHIN GROUP (ORDER BY ns3_landno) as ns3_landno,
       LISTAGG(photoair_province, ', ') WITHIN GROUP (ORDER BY photoair_province) as photoair_province,
       LISTAGG(photoair_number, ', ') WITHIN GROUP (ORDER BY photoair_number) as photoair_number,
       LISTAGG(photoair_page, ', ') WITHIN GROUP (ORDER BY photoair_page) as photoair_page,
	  sysdate as nowday
from lnmrtgmaster
where coop_id = $P{coopid}
and assettype_code = '03'
and mrtgmast_no = $P{mrtgmast_no}
and member_no = $P{member_no}
group by coop_id,
mrtgmast_no) ns3
where ns3.mrtgmast_no = lnm.mrtgmast_no
and lnm.coop_id = $P{coopid}
and lnm.assettype_code = '03'
and lnm.member_no = $P{member_no}
and lnm.mrtgmast_no = $P{mrtgmast_no}]]>
	</queryString>
