	npl.SETTING_PAYMENT,
	npl.FOLLOW_SEQ,
npl.int_lastcal,
	mem.COOP_ID,
	mem.MEMBER_NO,
	mem.PRENAME_CODE,
	mem.MEMB_NAME,
	mem.MEMB_SURNAME,
	 mem.membtype_code,
	mem.MEMBGROUP_CODE,
	mem.BIRTH_DATE,
n_pk_lnnpl.of_diff_year_month_thai($P{adtm_workdate}, mem.BIRTH_DATE) as BIRTH_AGE,
	mem.MEMBER_DATE,
n_pk_lnnpl.of_diff_year_month_thai($P{adtm_workdate}, mem.MEMBER_DATE) as MEMBER_AGE,
	mem.WORK_DATE,
n_pk_lnnpl.of_diff_year_month_thai($P{adtm_workdate}, mem.WORK_DATE) as WORK_AGE,
mem.card_person,
mem.salary_id,
mem.salary_amount,
mem.addr_no,
mem.addr_moo,
mem.addr_soi,
mem.addr_village,
mem.addr_road,
mem.tambol_code,
mem.amphur_code,
mem.province_code,
mem.addr_postcode,
mem.addr_phone,
mem.addr_mobilephone,
pv.province_desc,
dt.district_desc,
tb.tambol_desc,
	pre.PRENAME_DESC,
	mgr.MEMBGROUP_DESC,
	loan.LOANAPPROVE_DATE,
	loan.PRINCIPAL_ARREAR,
	loan.INTEREST_ARREAR,
	loan.INTYEAR_ARREAR,
	loan.INTEREST_ACCUM,
	loan.INTACCUM_LASTYEAR,
	loan.INTSET_ARREAR,
loan.lastpayment_date,
loan.principal_balance,
loan.lastcalint_date,
	coop.COOP_NAME,
	lawtype.LAWTYPE_DESC,
	lawtypeold.LAWTYPE_DESC as LAWTYPE_DESC_OLD,
	fm.advance_amt,
fm.description,
fm.case_blackno,
	fd.done,
fd.follow_date,
	fd.topic,
	fd.FOLLOW_DESC
FROM
	LNNPLMASTER npl
	INNER JOIN MBMEMBMASTER mem ON npl.coop_id = mem.coop_id  AND npl.MEMBER_NO = mem.member_no
	INNER JOIN MBUCFPRENAME pre ON mem.prename_code = pre.prename_code
	INNER JOIN MBUCFMEMBGROUP mgr ON mem.membgroup_code = mgr.membgroup_code
	INNER JOIN LNCONTMASTER loan ON npl.coop_id = loan.coop_id AND npl.loancontract_no = loan.loancontract_no
	INNER JOIN LNNPLFOLLOWDETAIL fd ON npl.coop_id = fd.coop_id AND npl.follow_seq = fd.follow_seq AND npl.member_no = fd.member_no
	INNER JOIN LNNPLFOLLOWMASTER fm ON npl.coop_id = fm.coop_id AND npl.follow_seq = fm.follow_seq AND npl.member_no = fm.member_no
	INNER JOIN CMCOOPMASTER coop ON npl.coop_id = coop.coop_id
	INNER JOIN LNUCFNPLLAWTYPE lawtype ON npl.coop_id = lawtype.coop_id AND npl.lawtype_code = lawtype.lawtype_code
	INNER JOIN LNUCFNPLLAWTYPE lawtypeold ON npl.coop_id = lawtypeold.coop_id AND npl.lawtype_code_old = lawtypeold.lawtype_code
	LEFT JOIN MBUCFPROVINCE pv ON mem.province_code = pv.province_code
	LEFT JOIN MBUCFDISTRICT dt ON mem.amphur_code = dt.district_code
	LEFT JOIN MBUCFTAMBOL tb ON mem.tambol_code = tb.tambol_code
WHERE
     npl.coop_id = $P{as_coopid}
     and npl.loancontract_no = $P{as_contno}
ORDER BY
     fd.done ASC,
     fd.follow_date DESC]]>
