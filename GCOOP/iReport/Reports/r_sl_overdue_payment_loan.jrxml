	<queryString>
		<![CDATA[select
	s.member_no,
	s.operate_date,
	s.kpslip_no as adjslip_no,
	m.salary_id,
	ft_getmemname(s.coop_id,s.member_no) as full_name,
	ft_memgrp(s.coop_id,m.membgroup_code) as membgroup,
	sum(decode(sd.keepitemtype_code,'S01',sd.adjust_itemamt,0)) as shr_adj,
	sum(decode(lt.loangroup_code,'01',sd.adjust_prnamt,0)) as prnemr,
	sum(decode(lt.loangroup_code,'01',sd.adjust_intamt,0)) as intemr,
	sum(decode(lt.loangroup_code,'02',(CASE WHEN l.loantype_code not in ('19','22') THEN sd.adjust_prnamt   ELSE 0 END),0)) as prnnor,
	sum(decode(lt.loangroup_code,'02',(CASE WHEN l.loantype_code not in ('19','22') THEN sd.adjust_intamt   ELSE 0 END),0)) as intnor,
	sum(decode(l.loantype_code,'19',sd.adjust_prnamt,0)) as prn19,
	sum(decode(l.loantype_code,'19',sd.adjust_intamt,0)) as int19,
	sum(decode(l.loantype_code,'22',sd.adjust_prnamt,0)) as prn22,
	sum(decode(l.loantype_code,'22',sd.adjust_intamt,0)) as int22,
	sum(decode(lt.loangroup_code,'03',sd.adjust_prnamt,0)) as prnspc,
	sum(decode(lt.loangroup_code,'03',sd.adjust_intamt,0)) as intspc
from kpmastreceive s inner join kpmastreceivedet sd on s.kpslip_no=sd.kpslip_no
	left join lncontmaster l on sd.loancontract_no=l.loancontract_no
	left join lnloantype lt on l.loantype_code =lt.loantype_code
	inner join mbmembmaster m on s.member_no=m.member_no
where s.member_no=$P{as_membno}
and s.coop_id=$P{as_coopid}
	and sd.adjust_itemamt > 0
group by
s.member_no,
s.operate_date,
s.kpslip_no,
m.salary_id,
ft_getmemname(s.coop_id,s.member_no) ,
ft_memgrp(s.coop_id,m.membgroup_code)]]>
	</queryString>
