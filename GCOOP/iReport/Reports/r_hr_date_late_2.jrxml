	<queryString>
		<![CDATA[SELECT
  he.emp_no,
  mp.prename_desc||he.emp_name||'  '||he.emp_surname as emp_name,
  (
  SELECT
    sum(nvl(hl.TOTALDAY,0))
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code
and hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '001'
  ) As leave1,
  (
  SELECT
    count(hl.leave_from)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code
and hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '001'
  ) As leave12,
  (
  SELECT
    sum(hl.TOTALDAY)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '002'
  ) As leave2,
  (
  SELECT
    count(hl.leave_from)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '002'
  ) As leave22,
  (
  SELECT
    sum(hl.TOTALDAY)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
 and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code between '001' and '002'
  ) As leave3,
  (
  SELECT
    count(hl.leave_from)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code between '001' and '002'
  ) As leave32,
  (
  SELECT
    sum(hl.TOTALDAY)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
 and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '006'
  ) As leave6,
  (
  SELECT
    count(hl.leave_from)
  FROM hrlogleave hl,
       hrucfleavecode hlc
  where hl.leave_code = hlc.leave_code and
        hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '006'
  ) As leave62,
  (
  SELECT
       sum(hl.totaltime)
  FROM hrlogleave hl
  where
        hl.emp_no = he.emp_no
and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '004'
  ) As leave4,
  (
  SELECT
       count(hl.OPERATE_DATE)
  FROM hrlogleave hl
  where
        hl.emp_no = he.emp_no
 and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '004'
  ) As leave42,
  (
  SELECT
    COUNT(hr.late_date)
  FROM hrloglate hr
  where hr.emp_no = he.emp_no
 and hr.late_date between $P{date1} and $P{date2}
  ) As late,
  (
  SELECT
       TO_NUMBER(substr(to_char(sum(hl.totaltime),'999.99'),-2))
  FROM hrlogleave hl
  where
        hl.emp_no = he.emp_no
 and hl.leave_from between $P{date1} and $P{date2}
        and hl.leave_code = '004'
  ) As leave43

FROM hremployee he,
     mbucfprename mp
where
     he.prename_code = mp.prename_code and
     he.emp_status = 1 and
     he.emptype_code = '02'
     and he.coop_id = $P{as_coopid}
order by he.emp_no]]>
	</queryString>
