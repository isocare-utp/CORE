	<queryString>
		<![CDATA[select hem.emp_no,
hem.salary_id,
mup.prename_desc,
hem.emp_name,
hem.emp_surname,
hup.pos_desc,
hul.deptline_desc,
hem.birth_date,
hem.work_date,
hem.contain_date
from hremployee hem,
mbucfprename mup,
hrucfdeptgrp hug,
hrucfdeptline hul,
hrucfposition hup
where hem.prename_code = mup.prename_code
and hem.deptgrp_code = hug.deptgrp_code
and hug.deptline_code = hul.deptline_code
and hem.pos_code = hup.pos_code
and hem.coop_id = $P{as_coopid}
and trim(hem.emp_no) between trim($P{as_sempno}) and trim($P{as_eempno})
order by hem.emp_no]]>
	</queryString>
