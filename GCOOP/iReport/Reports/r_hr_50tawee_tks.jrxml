	</parameter>
	<parameter name="as_sempno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_eempno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     he.emp_no,
     mp.prename_desc||he.emp_name||'  '||he.emp_surname as emp_name,
     he.id_card,
     (SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'P15'
and substr(hr.payroll_period,0,4) = $P{as_period}
      and hr.EMP_NO between $P{as_sempno} and $P{as_eempno}
) as P15,
     (SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'P12'
and substr(hr.payroll_period,0,4) = $P{as_period}
      and hr.EMP_NO between $P{as_sempno} and $P{as_eempno}
) as P12,
    (SELECT sum(hd.ITEM_AMT)
from HRPAYROLLDET hd,hrpayroll hr
where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
and hd.SALITEM_CODE = 'R01'
and substr(hr.payroll_period,0,4) = $P{as_period}
      and hr.EMP_NO between $P{as_sempno} and $P{as_eempno}
) as R01
FROM
     "HREMPLOYEE" he
     INNER JOIN MBUCFPRENAME mp ON he."PRENAME_CODE" = mp."PRENAME_CODE"
     FULL OUTER JOIN hrpayroll hr ON he.emp_no = hr.EMP_NO
     FULL OUTER JOIN cmcoopconstant cs ON he.coop_id = cs.coop_no
where

      hr.coop_id = $P{as_coopid}
      and substr(hr.payroll_period,0,4) = $P{as_period}
      and hr.EMP_NO between $P{as_sempno} and $P{as_eempno}

ORDER BY
     he.emp_no ASC]]>
	</queryString>
