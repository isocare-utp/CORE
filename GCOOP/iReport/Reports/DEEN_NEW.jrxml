	<queryString>
		<![CDATA[select
deen.land_ravang, deen.land_landno, deen.land_survey, deen.land_docno,
deen.land_bookno, deen.land_pageno,deen.pos_tambol, deen.pos_amphur,
deen.pos_province, deen.size_rai, deen.size_ngan, deen.size_wa,lnm.mrtg_name1,
replace( to_char( lnm.mrtg_personid1 , '9,9999,99999,99,9' ) , ',' , '-' ) ,
lnm.mrtg_name2, lnm.mrtg_personid2,lnm.mrtg_age1, lnm.mrtg_nationality1,
lnm.mrtg_citizenship1, lnm.mrtg_parentname1,lnm.mrtg_matename1,lnm.mrtg_village1,
lnm.mrtg_address1, lnm.mrtg_soi1, lnm.mrtg_road1, lnm.mrtg_moo1, lnm.mrtg_tambol1,
lnm.mrtg_amphur1, lnm.mrtg_province1,lnm.mortgage_grtname, lnm.autzd_name, lnm.autzd_age,
lnm.autzd_nationality, lnm.autzd_citizenship, lnm.autzd_parentname,lnm.autzd_village,
lnm.autzd_address, lnm.autzd_moo, lnm.autzd_tambol, lnm.autzd_amphur, lnm.autzd_province,
lnm.mortgagesum_amt, ft_readtbaht(lnm.mortgagesum_amt), case mortgage_type when 0 then 1 when 2 then lnm.mortgage_landnum else null end,case lnm.assettype_code when '03' then 'ก.' end, lnm.land_office, ftcnvtdate(lnm.mortgage_date, 3),deen.nowday
from lnmrtgmaster lnm,
(
select coop_id,
       mrtgmast_no,
       LISTAGG(size_rai, ', ') WITHIN GROUP (ORDER BY size_rai) as size_rai ,
       LISTAGG(size_ngan, ', ') WITHIN GROUP (ORDER BY size_ngan) as size_ngan ,
       LISTAGG(size_wa, ', ') WITHIN GROUP (ORDER BY size_wa) as size_wa ,
       LISTAGG(pos_tambol, ', ') WITHIN GROUP (ORDER BY pos_tambol) as pos_tambol ,
       LISTAGG(pos_amphur, ', ') WITHIN GROUP (ORDER BY pos_amphur) as pos_amphur ,
       LISTAGG(pos_province, ', ') WITHIN GROUP (ORDER BY pos_province) as pos_province ,
       LISTAGG(land_docno, ', ') WITHIN GROUP (ORDER BY land_docno) as land_docno ,
       LISTAGG(land_ravang, ', ') WITHIN GROUP (ORDER BY land_ravang) as land_ravang ,
       LISTAGG(land_survey,  ', ') WITHIN GROUP (ORDER BY land_survey) as land_survey ,
       LISTAGG(land_landno, ', ') WITHIN GROUP (ORDER BY land_landno) as land_landno ,
       LISTAGG(land_bookno, ', ') WITHIN GROUP (ORDER BY land_bookno) as land_bookno ,
       LISTAGG(land_pageno,  ', ') WITHIN GROUP (ORDER BY land_pageno) as land_pageno,
       sysdate as nowday
from lnmrtgmaster
where coop_id = $P{coopid}
and assettype_code = '01'
and trim(mrtgmast_no) =  $P{mrtgmast_no}
and member_no = $P{member_no}
group by coop_id,mrtgmast_no
) deen
where deen.mrtgmast_no = lnm.mrtgmast_no
 and deen.coop_id = lnm.coop_id
and lnm.coop_id = $P{coopid}
and lnm.assettype_code = '01'
and member_no = $P{member_no}
and trim(lnm.mrtgmast_no) =  $P{mrtgmast_no}]]>
	</queryString>
