	<queryString>
		<![CDATA[SELECT MBUCFPRENAME.PRENAME_DESC+''+MBMEMBMASTER.MEMB_NAME+' '+ MBMEMBMASTER.MEMB_SURNAME as fullname,
        LNREQLOAN.MEMBER_NO,
        LNREQLOAN.LOANTYPE_CODE,
        LNREQLOAN.LOANREQUEST_DOCNO,
        LNREQLOAN.LOANPERMISS_AMT,
        LNREQLOAN.LOANREQUEST_AMT,
        LNREQLOAN.LOANPAYMENT_TYPE,
        LNREQLOAN.PERIOD_PAYMENT,
        MBMEMBMASTER.MEMB_NAME,
        MBMEMBMASTER.MEMB_SURNAME,
        MBMEMBMASTER.MEMBGROUP_CODE,
        MBUCFMEMBGROUP.MEMBGROUP_DESC,
        CMCOOPCONSTANT.COOP_NAME,
        CMCOOPCONSTANT.MANAGER,
        LNREQLOAN.LOANCONTRACT_NO,
        MBMEMBMASTER.MEMBER_NO,
        MBMEMBMASTER.SALARY_ID,
        MBUCFPOSITION.POSITION_DESC,
'TO_CHAR(LNREQLOAN.APPROVE_DATE, dd MON yyyy, NLS_CALENDAR=''THAI BUDDHA'' NLS_DATE_LANGUAGE=THAI) 'as dateAPP,
        LNREQLOAN.LOANAPPROVE_AMT,
'ftreadtbaht( LNREQLOAN.LOANAPPROVE_AMT) 'as LOANAPPROVE_TBAHT
   FROM CMCOOPCONSTANT,
		MBMEMBMASTER LEFT JOIN LNREQLOAN ON LNREQLOAN.COOP_ID = MBMEMBMASTER.COOP_ID
		AND LNREQLOAN.MEMBER_NO = MBMEMBMASTER.MEMBER_NO
		LEFT JOIN LNLOANTYPE ON LNREQLOAN.LOANTYPE_CODE = LNLOANTYPE.LOANTYPE_CODE
		LEFT JOIN LNCFLOANINTRATEDET ON LNCFLOANINTRATEDET.LOANINTRATE_CODE = LNLOANTYPE.INTTABRATE_CODE
		LEFT JOIN LNUCFLOANOBJECTIVE ON LNREQLOAN.LOANOBJECTIVE_CODE = LNUCFLOANOBJECTIVE.LOANOBJECTIVE_CODE
		AND LNUCFLOANOBJECTIVE.LOANTYPE_CODE  =LNREQLOAN.LOANTYPE_CODE
		LEFT JOIN MBUCFMEMBGROUP ON MBUCFMEMBGROUP.COOP_ID = MBMEMBMASTER.COOP_ID
		AND MBUCFMEMBGROUP.MEMBGROUP_CODE = MBMEMBMASTER.MEMBGROUP_CODE
		LEFT JOIN MBUCFPRENAME ON MBUCFPRENAME.PRENAME_CODE = MBMEMBMASTER.PRENAME_CODE
		LEFT JOIN MBUCFPOSITION ON MBUCFPOSITION.POSITION_CODE = MBMEMBMASTER.POSITION_CODE

  WHERE (LNREQLOAN.APPROVE_DATE between LNCFLOANINTRATEDET.EFFECTIVE_DATE and LNCFLOANINTRATEDET.EXPIRE_DATE) and
         ( LNREQLOAN.COOP_ID = $P{as_coop_id}) AND
        ( LNREQLOAN.LOANREQUEST_DOCNO in ($P{as_loanrequest_docno}))]]>
	</queryString>
