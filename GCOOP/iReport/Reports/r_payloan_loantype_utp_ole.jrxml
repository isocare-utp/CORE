	<queryString>
		<![CDATA[SELECT
    (SELECT loantype_desc FROM lnloantype WHERE loantype_code = 10) AS loantype_desc,
    shrlontype_code,
    COUNT(payoutslip_no) AS count,
    SUM(payout_amt) AS payout_amt,
    SUM(payoutnet_amt) AS payoutnet_amt
FROM slslippayout
WHERE shrlontype_code = 10
AND slip_date between $P{date1} and $P{date2}
GROUP BY shrlontype_code
UNION
SELECT
    (SELECT loantype_desc FROM lnloantype WHERE loantype_code = 20) AS loantype_desc,
    shrlontype_code,
    COUNT(payoutslip_no) AS count,
    SUM(payout_amt) AS payout_amt,
    SUM(payoutnet_amt) AS payoutnet_amt
FROM slslippayout
WHERE shrlontype_code = 20
AND slip_date between $P{date1} and $P{date2}
GROUP BY shrlontype_code
UNION
SELECT
    (SELECT loantype_desc FROM lnloantype WHERE loantype_code = 33) AS loantype_desc,
    shrlontype_code,
    COUNT(payoutslip_no) AS count,
    SUM(payout_amt) AS payout_amt,
    SUM(payoutnet_amt) AS payoutnet_amt
FROM slslippayout
WHERE shrlontype_code = 33
AND slip_date between $P{date1} and $P{date2}
GROUP BY shrlontype_code
ORDER BY shrlontype_code]]>
	</queryString>
