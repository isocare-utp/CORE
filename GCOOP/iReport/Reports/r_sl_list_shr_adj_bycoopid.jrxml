	<queryString>
		<![CDATA[select
sa.adjslip_no,
sa.ref_recvperiod,
sa.member_no,
ft_getmemname(sa.coop_id,sa.member_no) as fullname,
decode(sd.slipitemtype_code,'SHR',sd.slipitem_desc,'LON',sd.loancontract_no,'') as itemtype,
sd.principal_adjamt,
sd.interest_adjamt,
sd.item_adjamt,mb.current_coopid,
decode(mb.current_coopid,'023001','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด',
'023002','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการปทุมธานี',
'023003','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการขอนแก่น',
'023004','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการลำปาง')
as coop
from slslipadjust sa,slslipadjustdet sd,mbmembmaster mb
where sa.coop_id = sd.coop_id
and sa.coop_id = mb.coop_id
and sa.member_no = mb.member_no
and sa.adjslip_no = sd.adjslip_no
and sd.slipitemtype_code = 'SHR'
and sa.slip_status = 1
and sa.pmx_status = 0
and ( sa.entry_bycoopid between $P{as_coopid} and $P{as_coop})
and mb.membgroup_code between $P{as_sgroup} and $P{as_egroup}
order by mb.membgroup_code,sa.member_no,sa.ref_recvperiod]]>
	</queryString>
