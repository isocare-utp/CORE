	<queryString>
		<![CDATA[select membgroup_code , membgroup_desc , member_no , prename_desc , memb_name , memb_surname, salary_id , work_date,current_coopid,coop_name
from(
select mug.membgroup_code , mug.membgroup_desc , m.member_no ,
mup.prename_desc , m.memb_name , m.memb_surname , m.salary_id , m.work_date,m.current_coopid,decode(m.current_coopid,
'023001','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด',
'023002','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการปทุมธานี',
'023003','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการขอนแก่น',
'023003','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการลำปาง')
as coop_name
from mbmembmaster m , mbucfprename mup , mbucfmembgroup mug ,
( select count(1) as cntadj from mbadjsalary where coop_id = $P{as_coopid} and operate_date = $P{adtm_operate} and posting_flag = 1 ) mac
where m.coop_id = mug.coop_id(+)
and m.membgroup_code = mug.membgroup_code(+)
and m.prename_code = mup.prename_code(+)
and m.current_coopid between $P{as_coopid} and $P{as_coopid2}
and m.member_date <= $P{adtm_operate}
and m.resign_status = 0
and mac.cntadj > 0
and not exists (
                    select 1 from mbadjsalary mas
                    where mas.entry_bycoopid between $P{as_coopid} and $P{as_coopid2}
                    and mas.operate_date = $P{adtm_operate}
	  and mas.posting_flag = 1
                    and mas.coop_id = m.coop_id
                    and mas.member_no = m.member_no
                )
union
select mug.membgroup_code , mug.membgroup_desc , m.member_no ,
mup.prename_desc , m.memb_name , m.memb_surname , m.salary_id , m.work_date,m.current_coopid,decode(m.current_coopid,
'023001','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด',
'023002','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการปทุมธานี',
'023003','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการขอนแก่น',
'023003','สหกรณ์ออมทรัพย์ไทยน้ำทิพย์ จำกัด ศูนย์บริการลำปาง')
as coop_name
from mbmembmaster m , mbucfprename mup , mbucfmembgroup mug ,
( select count(1) as cntadj from mbadjsalary where coop_id = $P{as_coopid} and operate_date = $P{adtm_operate} and posting_flag = 1 ) mac
where m.coop_id = mug.coop_id(+)
and m.membgroup_code = mug.membgroup_code(+)
and m.prename_code = mup.prename_code(+)
and m.current_coopid between $P{as_coopid} and $P{as_coopid2}
and m.member_date <= $P{adtm_operate}
and m.resign_date > $P{adtm_operate}
and m.resign_status = 1
and mac.cntadj > 0
and not exists (
                    select 1 from mbadjsalary mas
                    where mas.entry_bycoopid between $P{as_coopid} and $P{as_coopid2}
                    and mas.operate_date = $P{adtm_operate}
	  and mas.posting_flag = 1
                    and mas.coop_id = m.coop_id
                    and mas.member_no = m.member_no
                    )
)
order by member_no]]>
	</queryString>
