	<parameter name="as_coopid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="date1" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="date2" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
cc.coop_name,
ms.membgroup_code,
mbu.membgroup_desc,
total.member_no,
ft_getmbname(ms.coop_id,ms.member_no) as full_name,
total.loancontract_no,
total.total_INT,
total.total_PRN,
total.total_WRT,
ms.expense_accid,total.entry_date
from (select
	mb.member_no,
	mb.loancontract_no,
	sum(case when mb.returnitemtype_code='INT' then mb.return_amount else 0 end ) as total_INT,
	sum(case when mb.returnitemtype_code='PRN' then mb.return_amount else 0 end ) as total_PRN,
	0  as total_WRT,
mb.entry_date,
          0 as type_num
	from
	mbmoneyreturn mb,
	lnloantype ln
	where
mb.shrlontype_code = ln.loantype_code
and mb.return_status = 8
and mb.returnitemtype_code in ('INT','PRN')
and mb.entry_date between  $P{date1} and $P{date2}
group by mb.member_no,mb.loancontract_no,mb.entry_date
UNION ALL
select
	mb.member_no,
	mb.loancontract_no,
	0 as total_INT,
	0 as total_PRN,
	mb.return_amount  as total_WRT,
mb.entry_date,
	1 as type_num
	from
	mbmoneyreturn mb
	where mb.return_status = 8
	and mb.returnitemtype_code='WRT'
	and mb.entry_date between  $P{date1} and $P{date2}
) total,
mbmembmaster ms,
mbucfmembgroup mbu,
cmcoopconstant cc
where total.member_no= ms.member_no
and ms.membgroup_code=mbu.membgroup_code
and ms.coop_id = $P{as_coopid}
and trim(cc.coop_no) = ms.coop_id
order by ms.membgroup_code,total.member_no,total.type_num]]>
	</queryString>
