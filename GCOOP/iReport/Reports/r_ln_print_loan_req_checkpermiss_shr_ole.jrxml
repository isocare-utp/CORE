	<queryString>
		<![CDATA[select
lnreqloanclr.loancontract_no ,
lncontmaster.intestnow_amt + lncontmaster.principal_balance as total,(select count(adjslip_no) from slslipadjust
where pmx_status = 1
and member_no = lncontmaster.member_no) as coun

from
lnreqloan LEFT JOIN lnreqloanclr on lnreqloan.coop_id = lnreqloanclr.coop_id  and lnreqloan.loanrequest_docno = lnreqloanclr.loanrequest_docno
LEFT JOIN lncontmaster on lnreqloanclr.loancontract_no = lncontmaster.loancontract_no

where
lnreqloan.coop_id = $P{coop_id} and
lnreqloan.loanrequest_docno = $P{loanrequest_docno} and
lnreqloanclr.clear_status = '1'
union
select
lnreqloanclrother.clrother_desc,
lnreqloanclrother.clrother_amt,(select count(adjslip_no) from slslipadjust
where pmx_status = 1
and member_no = lnreqloan.member_no)

from
lnreqloan LEFT JOIN lnreqloanclrother on lnreqloan.coop_id = lnreqloanclrother.coop_id  and lnreqloan.loanrequest_docno = lnreqloanclrother.loanrequest_docno

where
lnreqloan.coop_id = $P{coop_id} and
lnreqloan.loanrequest_docno = $P{loanrequest_docno} and
lnreqloanclrother.clear_status = '1'
union
select
lnreqloanmthexp.mthexp_desc,
lnreqloanmthexp.mthexp_amt,(select count(adjslip_no) from slslipadjust
where pmx_status = 1
and member_no = lnreqloan.member_no)

from
lnreqloan LEFT JOIN lnreqloanmthexp on lnreqloan.coop_id = lnreqloanmthexp.coop_id  and lnreqloan.loanrequest_docno = lnreqloanmthexp.loanrequest_docno

where
lnreqloan.coop_id = $P{coop_id} and
lnreqloan.loanrequest_docno = $P{loanrequest_docno} and
lnreqloanmthexp.clear_status = '1']]>
	</queryString>
