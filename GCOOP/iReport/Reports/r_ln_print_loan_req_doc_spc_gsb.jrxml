	<queryString>
		<![CDATA[select ft_getmbname(lr.coop_id,lr.member_no) as full_name,
mb.member_no,
mb.salary_id,
mb.mate_name,
lr.loanrequest_date,
TO_CHAR(lr.loanrequest_date,'dd') as day,
TO_CHAR(lr.loanrequest_date,'fmMonth') as month,
TO_CHAR(lr.loanrequest_date,'yyyy')+543 as year,
mb.card_person,
lr.loanrequest_docno,
lr.coop_id,
mb.Addr_No,
mb.Addr_Moo,
mb.Addr_Soi,
mb.Addr_Village,
mb.Addr_Road,
mb.Tambol_Code,
mb.Amphur_Code,
mb.Province_Code,
mb.Addr_Postcode,
mb.Addr_phone,
lr.loanapprove_amt,
ftreadtbaht(lr.loanapprove_amt) as loanapprove_tbaht,
ft_memgrp(lr.coop_id,mb.membgroup_code) as membgroup_desc,
trunc(Ft_Calage( birth_date , sysdate , 4 )) as birth,
pr.province_desc,
mt.tambol_desc,
md.district_desc,
lr.period_payment,
ftreadtbaht(lr.period_payment) as tperiod_payment,
lr.period_payamt,
lo.loanobjective_desc,
ld.interest_rate,
sysdate as nowdate,
nvl(mp.position_desc,mb.position_desc) as position_desc
from
lnreqloan lr,
mbmembmaster mb,
mbucfprovince pr,
mbucfdistrict md,
mbucftambol mt,
lnucfloanobjective lo,
lncfloanintratedet ld,
lnloantype lc,
mbucfposition mp
where
lr.member_no = mb.member_no
and lr.loantype_code = lc.loantype_code
and lc.inttabrate_code = ld.loanintrate_code
and (lr.approve_date between ld.effective_date and ld.expire_date)
and mb.province_code = pr.province_code
and mb.Amphur_Code = md.district_code
and mb.Tambol_Code = mt.tambol_code
and lr.loantype_code = lo.loantype_code
and lr.loanobjective_code = lo.loanobjective_code
and mb.position_code = mp.position_code
and lr.coop_id = mb.coop_id
and exists (select loanrequest_docno from lnspcreport where lr.loanrequest_docno = lnspcreport.loanrequest_docno and entry_date = (select max(entry_date) as max_date from lnspcreport) and entry_id = $P{as_entryid})
and lr.coop_id = $P{as_coopid}]]>
	</queryString>
