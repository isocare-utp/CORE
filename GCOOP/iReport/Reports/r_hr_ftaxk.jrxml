	<parameter name="as_coopid" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="as_period" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
      cs.coop_name,
      cs.coop_addr,
      cs.tambol,
      md.district_desc,
      mv.province_desc,
      cs.postcode,
      cs.coop_tel,
      cs.coop_fax,
      cs.vicemanager,
      cs.coop_taxid,
      (select hd.ITEM_AMT
      from HRPAYROLLDET hd
      where hr.PAYROLLSLIP_NO = hd.PAYROLLSLIP_NO
            and hd.coop_id = cs.coop_no
            and hd.SALITEM_CODE = 'P01') as item_amt,
      hr.salarybase_amt,
      (
      select count( hr.salarybase_amt)
      from hrpayroll hr
      where hr.coop_id = cs.coop_no and
      hr.coop_id = $P{as_coopid}
      and substr(hr.payroll_period,0,4) = $P{as_period} ) as count_no,
      (
      select sum(hr.salarybase_amt)
      from hrpayroll hr
      where hr.coop_id = cs.coop_no and
      hr.coop_id =  $P{as_coopid}
and substr(hr.payroll_period,0,4) = $P{as_period} ) as sumsalary,
      (
      select sum(hd.ITEM_AMT)
      from hrpayroll hr,
      hrpayrolldet hd
      where
      hd.SALITEM_CODE = 'P01' and
      hr.coop_id = $P{as_coopid}
      and substr(hr.payroll_period,0,4) = $P{as_period}  and
      hr.payrollslip_no = hd.payrollslip_no) as sumitem
,(select ch.prename||ch.human_name from cmcoophuman ch where ch.human_position = 'รองผู้จัดการสายงานการเงินและบัญชี') as human_name
     ,(select ch.human_position from cmcoophuman ch where ch.human_position = 'รองผู้จัดการสายงานการเงินและบัญชี' ) as human_position
from
      hrpayroll hr,
      cmcoopconstant cs,
      mbucfdistrict md,
      mbucfprovince mv
where
      cs.district_code = md.district_code and
      cs.province_code = mv.province_code and
      cs.coop_no = hr.coop_id and
      hr.coop_id = $P{as_coopid}
      and substr(hr.payroll_period,0,4) = $P{as_period}
order by hr.emp_no]]>
	</queryString>
